{
    "name": "ricardopedias/freep-console",
    "type": "tool",
    "description": "Camada para linha de comando do framework Freep.",
    "keywords": ["framework", "freep", "tool", "console"],
    "license": "MIT",
    "require": {
        "php": "^8.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Freep\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testes\\": "testes/"
        }
    },
    "bin": [
        "freep"
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts" : {
        "testar": "vendor/bin/phpunit --testdox"
    }
}
